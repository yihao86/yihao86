// 我要晒单弹窗
$(".thesun").on("click",".nosd .btn,.wantsd a",function(){

    if (Param.isLogin == 0) {
        toLogin();
        return false;
    }

    $.ajax({
        url: Url.getUploadSignUrl,
        dataType: 'JSON',
        async: true,
        type: 'GET',
        success: function (res) {
            createUploader(res);
        }
    });
    $("#mall-win,#mall-win .upload-sd").removeClass("hide");
});


//创建晒单
function createUploader(res) {
    var uploader,
        $message = $(".message"),
        $pic = $("#upbtn");

    uploader = WebUploader.create({
        swf: Url.webUploaderSwf,
        server: Url.webUploaderApiUrl,

        pick: {id: $("#upbtn"), multiple: true},
        auto: true,

        fileNumLimit: 3,
        accept: {
            title:       'Images',
            extensions: 'jpg,jpeg,png,gif,bmp',
            mimeTypes: 'image/jpg,image/jpeg,image/png,image/gif,image/bmp'
        },
        formData: {
            policy: res.policy,
            signature: res.sign
        }
    });


    // 添加队列时
    uploader.on('fileQueued', function (file) {
        if($(".img-show .imglist").length < 3){
            $message.html('正在上传...');
        }
    });

    //上传成功
    uploader.on('uploadSuccess', function (file, response) {

        if($(".img-show .imglist").length < 3){
            var $li = '<div class="imglist fl" data-img="'+ response.url +'" style="background-image: url(//pic.huke88.com/' + response.url + '!/both/250x250);"><i class="del"></i></div>';
            $('#upbtn').before($li);

            var pic = $("#uploadComment .picture").val();
            if(pic == ''){
                $("#uploadComment .picture").val(response.url);
            }else{
                $("#uploadComment .picture").val(pic + '+' + response.url);
            }
        }

        if($(".img-show .imglist").length >= 3) {
            $(".webuploader-pick").html('最多3张');
        }

        $.ajax({
            url: Url.getUploadSignUrl,
            dataType: 'JSON',
            async: true,
            type: 'GET',
            success: function (res) {
                createUploader(res);
            }
        });

    });

    //上传完成
    uploader.on('uploadComplete', function (file) {
        $message.html(' ');
        // $('.webuploader-pick').parent('div').remove();
    });

    //上传失败
    uploader.on('uploadError', function (file, reason) {
        if (reason === 'http') {
            layer.alert('上传速度太快了，休息一会');
        } else {
            layer.alert('上传出错');
        }

        $.ajax({
            url: Url.uploadSignUrl,
            dataType: 'JSON',
            async: true,
            type: 'GET',
            success: function (res) {
                createUploader(res);
            }
        });
    });

    //错误提示
    uploader.on('error', function (msg) {
        var $message = '上传出错';
        switch (msg) {
            case 'Q_EXCEED_SIZE_LIMIT':
                $message = '体积过大';
                break;
            case 'F_DUPLICATE':
                $message = '文件不能重复上传';
                break;
            case 'Q_EXCEED_NUM_LIMIT ':
                $message = '文件过多';
                break;
            case 'Q_TYPE_DENIED ':
                $message = '文件类型不满足';
                break;
        }

        layer.alert($message);

        $.ajax({
            url: Url.uploadSignUrl,
            dataType: 'JSON',
            async: true,
            type: 'GET',
            success: function (res) {
                createUploader(res);
            }
        });
    });

    //删除文件
    $(".img-show .imglist .del").on('click', function () {
        var $pic = $("#uploadComment .picture"),
            val = $(this).closest('.imglist').attr('data-img'),
            $newPic = '';
        $(this).closest('.imglist').remove();


        $.each($pic.val().split('+'),function (k, v) {
            if(v != val){
                if($newPic == ''){
                    $newPic = v;
                }else{
                    $newPic += '+' + v;
                }
            }
        } );

        $pic.val($newPic);

        uploader.refresh();
        if($(".imglists").children().length < 3) {
            $(".webuploader-pick").html('点击上传图片');
        }
    });
}


//提交晒单请求
$(".upload-sd").on('click', '.tj', function () {

    var $form = $("#uploadComment"),
        $comment = $form.find('textarea'),
        $picture = $form.find('.picture');

    var comment = $comment.val(),
        picture = $picture.val();

    if(comment == '' || picture == ''){

        if(comment == ''){
            $comment.css('border', '1px solid red');
        }
        if(picture == ''){
            $picture.addClass('hide');
            $(".message").html('至少上传一张图片').css('color', 'red');
        }
        return false;
    }

    $.ajax({
        type: "post",
        url: Url.commentAdd,
        data: {
            comment     : comment,
            picture     : picture,
            '_csrf-frontend': $('meta[name="csrf-token"]').attr('content')
        },
        dataType: "json",
        success: function (data) {
            if(data.error === 0){
                $(".upload-sd"),$("#mall-win").addClass('hide');
                window.location.reload();
            }
        },
        error:function () {
            console.log('请联系管理员');
        }
    });
});

//清楚错误提示
$("textarea").on('focus', function () {
    $("#uploadComment textarea").css('border', '1px #e8e8e8 solid');
    $(".message").html(' ').css('color', '#666');
})

// 通用关闭弹窗关闭
$("#mall-win").on("click",".close",function(){
    $(this).parents(".win-m").addClass("hide");
    $("#mall-win").addClass("hide");
});

// 浏览晒单图片
$("#hmall .evaluate-list").on("click",".list-r",function(){
    var $container = $(".evaluate-list-m");
    var list = $(this).closest('.list');

    var left = list.find('.list-l').html();

    $container.find('.list-l').html(left);
    var pictures = $(this).attr('data-pictures');

    var arr = pictures.split('+');
    var html = '';

    $.each(arr , function (k, v) {
        html += '<li class="slide-item" style="background-image: url(' + v +')"><span class="num">图'+ (k+1) +'</span></li>';
    });
    $container.find(".list-r ul").html(html);

    // 商城晒单轮播图
    $(".show-img").slide({
        mainCell: ".bd ul",
        autoPlay: true,
        trigger: "click",
        interTime: 4000
    });

    $("#mall-win,#mall-win .evaluate-list").removeClass("hide");
});

