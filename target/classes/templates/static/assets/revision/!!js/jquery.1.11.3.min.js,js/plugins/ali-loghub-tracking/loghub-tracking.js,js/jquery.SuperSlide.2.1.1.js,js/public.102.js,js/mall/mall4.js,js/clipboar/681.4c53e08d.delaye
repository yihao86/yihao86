/**
 * 底部友链左右滑动
 */
function friendSlide() {
    var cm = $('.friends-link .slide-m').width();
    sm = $('.friends-link .link-m').width();
    dm = sm - cm;
    if (sm > cm) {
        $('.friends-link .link-m').css({
            left: '0',
        });
        $('.link-t .next').show();
        $('.link-t').on('click', '.next', function () {
            $('.friends-link .link-m').css({
                left: -dm,
            });
            $(this).hide();
            $('.prev').show();
        });
        $('.link-t').on('click', '.prev', function () {
            $('.friends-link .link-m').css({
                left: '0'
            });
            $(this).hide();
            $('.next').show();
        });
    } else {
        $('.link-t .next').hide();
    }

}

friendSlide();
$(window).resize(function () {
    friendSlide();
});

