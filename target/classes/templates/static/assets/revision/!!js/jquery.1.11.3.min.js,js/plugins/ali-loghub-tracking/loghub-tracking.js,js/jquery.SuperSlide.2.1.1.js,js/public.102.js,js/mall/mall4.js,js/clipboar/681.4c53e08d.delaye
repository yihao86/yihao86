function tab(type) {
    var goods = $(".goods");
    if(type === 1){
        $.ajax({
            type: "get",
            url: Url.goods,
            data: { },
            dataType: "json",
            success: function (response) {
                // response 204
                goods.removeClass('hide');
            }
        });
    }
}

//输入判定
function onInputs(_self) {
    $(_self).closest('#uploadComment').find('em').text($(_self).val().length);
}
