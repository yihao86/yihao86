$(function () {

    if (Param.loginPop == 2) {
        //账号已在别处登录提示
        layer.confirm('<p class="fz20-c3">小虎提醒</p><p class="fz16-c6">您的账号已在别处登录</p><p class="fz16-c6">如非本人操作请立即修改密码哦~</p>',
