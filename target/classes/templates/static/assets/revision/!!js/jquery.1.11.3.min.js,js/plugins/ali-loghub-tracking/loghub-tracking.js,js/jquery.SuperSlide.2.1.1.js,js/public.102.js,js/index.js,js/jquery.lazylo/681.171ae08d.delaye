        selectPlayer: function () { //true HTML5 false  flash

            var ua = navigator.userAgent;

            var ipad = ua.match(/(iPad).*OS\s([\d_]+)/),

                isIphone = !ipad && ua.match(/(iPhone\sOS)\s([\d_]+)/),

                isAndroid = ua.match(/(Android)\s+([\d.]+)/),

                isMobile = isIphone || isAndroid;
            if (isMobile) {
                return false;
            }

            var userAgent = navigator.userAgent; //userAgent
            var isOpera = userAgent.indexOf("Opera") > -1;
            if (isOpera) {//Opera
                return true;
            }
            if (userAgent.indexOf("Firefox") > -1) {//Firefox
                return true;
            }
            if (userAgent.indexOf("Chrome") > -1) {//
                var arr = navigator.userAgent.split(' ');
                var chromeVersion = '';
                for (var i = 0; i < arr.length; i++) {
                    if (/chrome/i.test(arr[i])) {
                        chromeVersion = arr[i];
                    }
                }
                if (chromeVersion) {
                    var version = Number(chromeVersion.split('/')[1].split('.')[0]);
                    if (version >= 40) {
                        return true;
                    }
                }
                return false;
            }
            if (userAgent.indexOf("Safari") > -1) {//safari
                return true;
            } //Safari

            var isEdge = userAgent.indexOf("Edge") > -1 && !isIE; //IEEdge
            if (isEdge) {
                return true;
            }

            var isIE11 = userAgent.indexOf('Trident') > -1 && userAgent.indexOf("rv:11.0") > -1;
            return false;
        },
