            if (countdown_valid == 0) {
                obj.removeClass("ed");
                obj.attr("disabled", false);
                obj.val("");
                countdown_valid = 60;
                return;
            } else {
                obj.addClass("ed");
                obj.attr("disabled", true);
                obj.val(countdown_valid + "s");
                countdown_valid--;
            }
