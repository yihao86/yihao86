            if ($phone == '' || !checkTelNum($phone)) {
                errorMessage($phone_error, true, '');
                return false;
            } else {
                errorMessage($phone_error, false);
            }

            if ($code == '' || $code.length != 4) {
                errorMessage($code_error, true, '');
                return false;
            } else {
                errorMessage($code_error, false);
            }

            if ($sms == '' || $sms.length != 4) {
                errorMessage($sms_error, true, '');
