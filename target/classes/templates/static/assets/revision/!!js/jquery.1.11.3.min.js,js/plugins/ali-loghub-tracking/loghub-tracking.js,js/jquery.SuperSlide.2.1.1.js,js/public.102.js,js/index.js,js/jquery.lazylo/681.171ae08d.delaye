                                if(Param.huke_gold){
                                    layer.confirm('<p class="fz16-c3"><span class="cf70">'+res.gold+'</span>/40</p><p class="fz16-c6"></p>',
                                        {
                                            btn: [''],//
                                            btnColor: ['2'] // 1 2
                                        },
                                        function() {
                                            layer.close(layer.index);//
                                        }
                                    );
                                    Param.huke_gold = 0;
                                }else{
                                    layer.msg(' ');
                                }
                            } else {
                                layer.msg('');
